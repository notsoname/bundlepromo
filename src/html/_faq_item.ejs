<div class="faq-item item-expandable is_collapsed">
    <div class="faq-question item-expandable-trigger">
        <h3><span class="index"><%- (index) %>.</span><%- t(`QUESTION_${index}`) %></h3>
        <svg class="arrow" viewBox="0 0 32 32">
            <rect width="32" height="32" fill="none"/>
            <path d="M12.116,14.169,0,1.872,1.9,0,12.116,10.37,22.333,0l1.9,1.872Z" transform="translate(3.884 10.249)"/>
        </svg>
    </div>
    <div class="faq-answer">
        <p><%- t(`ANSWER_${index}`) %></p>
    </div>
</div>
